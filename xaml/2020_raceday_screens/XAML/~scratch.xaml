<!-- ****************************************************************** -->
<!-- *                           Standard                             * -->
<!-- *        lane display for Race Data panel in RaceDay file        * -->
<!-- *                                                                * -->
<!-- ****************************************************************** -->
<!-- *  Usage: 1) Copy this code into the RaceDay file                * -->
<!-- *         2) Replace all instances of <Lane#> with lane number   * -->  
<!-- ****************************************************************** -->

                        <!-- ********************** -->
                        <!-- *    Lane <Lane#> Start    * -->
                        <!-- ********************** -->
                        <!-- Name -->
                        <Label Grid.Row="<Lane#>" Grid.Column="0" Name="Nickname_Position<Lane#>_1" Style="{StaticResource laneData}" />
                        <Viewbox MaxWidth="335" MaxHeight="168" Grid.Row="<Lane#>" Grid.Column="0">
                            <Label Name="Nickname_Position<Lane#>_2" Style="{StaticResource laneDataLayer2}" />
                        </Viewbox>
                        <!-- Lap -->
                        <Label Name="Lap_Position<Lane#>_1" Grid.Row="<Lane#>" Grid.Column="1" Style="{StaticResource laneData}" />
                        <Viewbox Grid.Row="<Lane#>" Grid.Column="1" MaxHeight="168" MaxWidth="96">
                            <Label Name="Lap_Position<Lane#>_2" Style="{StaticResource laneDataLayer2}" />
                        </Viewbox>
                        <!-- Lap:  Drift lap notification (inside lap cell) -->
                        <Grid Grid.Row="<Lane#>" Grid.Column="1" Background="Transparent">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="50" />
                                <ColumnDefinition Width="50" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="57" />
                                <RowDefinition Height="57" />
                                <RowDefinition Height="57" />
                            </Grid.RowDefinitions>
                            <Viewbox Grid.Row="2" Grid.Column="1" MaxHeight="53" MaxWidth="46">
                                <Image Name="DriftLap_Position<Lane#>_1" Stretch="Uniform"/>
                            </Viewbox>
                        </Grid>
                        <!-- Lap:  Add lap sections button (click inside lap cell) -->
                        <Button Name="AddLapSections_Position<Lane#>_1" Grid.Row ="<Lane#>" Grid.Column="1" Focusable="False" IsTabStop="False" Opacity="0" ToolTip="Click to add laps/sections" />
                        <!-- Lap Time -->
                        <Label Name="LapTime_Position<Lane#>_1" Grid.Row="<Lane#>" Grid.Column="2" Style="{StaticResource laneData}" />
                        <Viewbox Grid.Row="<Lane#>" Grid.Column="2" MaxHeight="168" MaxWidth="191">
                            <Label Name="LapTime_Position<Lane#>_2" DataContext="'decimals':3" Style="{StaticResource laneDataLayer2}" />
                        </Viewbox>
                        <StackPanel Grid.Row="<Lane#>" Grid.Column="2" HorizontalAlignment="Right" Margin="4" Orientation="Horizontal" VerticalAlignment="Bottom">
                            <Image Width="15" Height="20" Name="SelfPerformance_Position<Lane#>_1" Margin="1,0" Stretch="Fill" />
                            <Image Width="15" Height="20" Name="HeatPerformance_Position<Lane#>_1" Margin="1,0" Stretch="Fill" />
                            <Image Width="15" Height="20" Name="TrackPerformance_Position<Lane#>_1" Margin="1,0" Stretch="Fill" />
                        </StackPanel>
                        <!-- Median Lap Time -->
                        <Label Name="MedianTime_Position<Lane#>_1" Grid.Row="<Lane#>" Grid.Column="3" Style="{StaticResource laneData}" />
                        <Viewbox Grid.Row="<Lane#>" Grid.Column="3" MaxHeight="168" MaxWidth="191">
                            <Label Name="MedianTime_Position<Lane#>_2" DataContext="'decimals':3" Style="{StaticResource laneDataLayer2}" />
                        </Viewbox>
                        <!-- Best Lap Time -->
                        <Label Name="BestLapTime_Position<Lane#>_1" Grid.Row="<Lane#>" Grid.Column="4" Style="{StaticResource laneData}" />
                        <Viewbox Grid.Row="<Lane#>" Grid.Column="4" MaxHeight="168" MaxWidth="191">
                            <Label Name="BestLapTime_Position<Lane#>_2" DataContext="'decimals':3" Style="{StaticResource laneDataLayer2}" />
                        </Viewbox>
                        <!-- ********************** -->
                        <!-- *    Lane <Lane#> End      * -->
                        <!-- ********************** -->




