<!-- ****************************************************************** -->
<!-- *                           Practice                             * -->
<!-- *        lane display for Race Data panel in RaceDay file        * -->
<!-- *                                                                * -->
<!-- ****************************************************************** -->
<!-- *  Usage: 1) Copy this code into the RaceDay file                * -->
<!-- *         2) Replace all instances of <Lane#> with lane number   * -->  
<!-- *         3) Replace <Lane# + 1> with lane number + 1            * -->  
<!-- *         4) Add Width="<width>" to button to set it's width     * -->  
<!-- ****************************************************************** -->

            <!-- ********************** -->
            <!-- *    Lane <Lane#> Start    * -->
            <!-- ********************** -->
            <Label Grid.Column="<Lane#>" Grid.Row="1" Name="BestLapTime_Lane<Lane#>_1" DataContext="'decimals':3" FontSize="54" HorizontalContentAlignment="Center" Style="{StaticResource practiceData}" />
            <Label Grid.Column="<Lane#>" Grid.Row="2" Name="LapTime_Lane<Lane#>_1" DataContext="'index':0,'decimals':3" FontSize="80" Style="{StaticResource practiceData}" />
            <Image Grid.Column="<Lane#>" Grid.Row="2" Name="SelfPerformance_Lane<Lane#>_1" Height="30" HorizontalAlignment="Right" Margin="0, 0, 10, 10" Stretch="Fill" VerticalAlignment="Bottom" Width="35" />
            <Label Grid.Column="<Lane#>" Grid.Row="3" Name="LapTime_Lane<Lane#>_2" DataContext="'index':1,'decimals':3" Style="{StaticResource practiceData}" />
            <Label Grid.Column="<Lane#>" Grid.Row="4" Name="LapTime_Lane<Lane#>_3" DataContext="'index':2,'decimals':3" Style="{StaticResource practiceData}" />
            <Label Grid.Column="<Lane#>" Grid.Row="5" Name="LapTime_Lane<Lane#>_4" DataContext="'index':3,'decimals':3" Style="{StaticResource practiceData}" />
            <Label Grid.Column="<Lane#>" Grid.Row="6" Name="LapTime_Lane<Lane#>_5" DataContext="'index':4,'decimals':3" Style="{StaticResource practiceData}" />
            <Label Grid.Column="<Lane#>" Grid.Row="7" Name="LapTime_Lane<Lane#>_6" DataContext="'index':5,'decimals':3" Style="{StaticResource practiceData}" />
            <Label Grid.Column="<Lane#>" Grid.Row="8" Name="LapTime_Lane<Lane#>_7" DataContext="'index':6,'decimals':3" Style="{StaticResource practiceData}" />
            <Label Grid.Column="<Lane#>" Grid.Row="9" Name="LapTime_Lane<Lane#>_8" DataContext="'index':7,'decimals':3" Style="{StaticResource practiceData}" />
            <Label Grid.Column="<Lane#>" Grid.Row="10" Grid.RowSpan="2" Name="Lap_Lane<Lane#>_1" FontSize="54" Style="{StaticResource practiceData}" />
            <Button Grid.Column="<Lane#>" Grid.Row="12" Name="ClearLane_<Lane# + 1>" Content="Clear Lane" ToolTip="Clear lap information for lane <Lane#>" DataContext="'key':'F<Lane#>'" CommandParameter="<Lane#>" Style="{StaticResource roundCornerLarge}" />
            <!-- ********************** -->
            <!-- *    Lane <Lane#> End      * -->
            <!-- ********************** -->



<!-- ****************************************************************** -->
<!-- *                         Leaderboard                            * -->
<!-- *      lane display for Race Data panel in leaderboard file      * -->
<!-- *                                                                * -->
<!-- ****************************************************************** -->
<!-- *  Usage: 1) Copy this code into the leaderboard file            * -->
