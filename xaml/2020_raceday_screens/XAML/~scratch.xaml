            <!-- Leaderboard Position <Lane#> -->
            <TextBlock Text="<Lane#>" Grid.Row="<Lane#>" Grid.Column="0" Style="{StaticResource leaderBoardText}" />
            <Viewbox MaxWidth="180" MaxHeight="25" MinHeight="25" Grid.Row="<Lane#>" Grid.Column="1" HorizontalAlignment="Left">
                <TextBlock Name="Nickname_RaceLeader<Lane#>_1" Margin="5,0" Style="{StaticResource leaderBoardText}" />
            </Viewbox>
            <Viewbox MaxWidth="70" MaxHeight="25" MinHeight="25" Grid.Row="<Lane#>" Grid.Column="2" HorizontalAlignment="Right">
                <TextBlock Name="Standing_RaceLeader<Lane#>_1" DataContext="'decimals':3" Style="{StaticResource leaderBoardText}" />
            </Viewbox>



<!-- ************************************************************************ -->
<!-- *                           Race Results                               * -->
<!-- *      results display for Race Data window in raceresults file        * -->
<!-- *                                                                      * -->
<!-- ************************************************************************ -->
<!-- *  Usage: 1) Copy this code into the raceresults file                  * -->
<!-- *         2) Replace all instances of <position#> with position number * -->
<!-- *         3) For position 1, add:                                      * -->
<!-- *           Foreground="Transparent"                                   * -->
<!-- *           to GapLeader_ and GapPosition_ labels                      * -->
<!-- *         4) For position 2, add:                                      * -->
<!-- *           Foreground="Transparent"                                   * -->
<!-- *           to GapPosition_ label                                      * -->
<!-- *         5) For odd numbered positions, add:                          * -->
<!-- *           Background="{StaticResource brushSecondaryLight}"          * -->
<!-- *           to StackPanel                                              * -->
<!-- ************************************************************************ -->

                    <!-- ************************** -->
                    <!-- *    Position <position#> Start    * -->
                    <!-- ************************** -->
                    <StackPanel Orientation="Horizontal" Height="40">
                        <Grid Width="75" >
                            <Viewbox MaxWidth="75"  MaxHeight="40" MinHeight="40">
                                <Label Name="Pos<position#>_1" Content="<position#>" HorizontalContentAlignment="Right" Style="{StaticResource RaceResultsData}" />
                            </Viewbox>
                        </Grid>
                        <Grid Width="50">
                            <Image Name="Avatar_RaceLeader<position#>_1" MaxWidth="45" MaxHeight="40" Stretch="Fill" />
                        </Grid>
                        <Grid Width="219">
                            <Viewbox MaxWidth="216"  MaxHeight="40" MinHeight="40">
                                <Label Name="Nickname_RaceLeader<position#>_1" HorizontalContentAlignment="Right" Style="{StaticResource RaceResultsData}" />
                            </Viewbox>
                        </Grid>
                        <Grid Width="100">
                            <Viewbox StretchDirection="DownOnly">
                                <Label Name="Standing_RaceLeader<position#>_1" HorizontalContentAlignment="Right" DataContext="'decimals':2" Style="{StaticResource RaceResultsData}" /> 
                            </Viewbox>
                        </Grid>
                        <Grid Width="100">
                            <Viewbox StretchDirection="DownOnly">
                                <Label Name="Lap_RaceLeader<position#>_1" HorizontalContentAlignment="Right" DataContext="'decimals':2" Style="{StaticResource RaceResultsData}" /> 
                            </Viewbox>
                        </Grid>
                        <Grid Width="110">
                            <Viewbox StretchDirection="DownOnly">
                                <Label Name="AverageTime_RaceLeader<position#>_1" MaxWidth="110" HorizontalContentAlignment="Right" DataContext="'decimals':3" Style="{StaticResource RaceResultsData}" /> 
                            </Viewbox>
                        </Grid>
                        <Grid Width="110">
                            <Viewbox StretchDirection="DownOnly">
                                <Label Name="BestLapTime_RaceLeader<position#>_1" MaxWidth="110" HorizontalContentAlignment="Right" DataContext="'decimals':3" Style="{StaticResource RaceResultsData}" /> 
                            </Viewbox>
                        </Grid>
                        <Grid Width="130">
                            <Viewbox StretchDirection="DownOnly">
                                <Label Name="GapLeader_RaceLeader<position#>_1" Content="" HorizontalContentAlignment="Right" DataContext="'decimals':3" Style="{StaticResource RaceResultsData}" /> 
                            </Viewbox>
                        </Grid>
                        <Grid Width="130">
                            <Viewbox StretchDirection="DownOnly">
                                <Label Name="GapPosition_RaceLeader<position#>_1" Content="" HorizontalContentAlignment="Right" DataContext="'decimals':3" Style="{StaticResource RaceResultsData}" /> 
                            </Viewbox>
                        </Grid>
